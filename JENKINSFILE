pipeline {
    agent any

    stages{
         stage('solo hago un echo'){
            steps{
                echo "primera tarea en mi pipeline"
                git 'https://github.com/Felipe-Hernandez-dev/helloworld'
                bat 'dir'
                echo WORKSPACE
            }
        }
          stage('BUILD'){
            steps{
                echo "Este Build no hace nada"
                bat 'hostname'
                bat 'whoami'
            }
        }  
    }                      
}